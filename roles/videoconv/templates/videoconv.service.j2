[Unit]
Description=videoconv video processing
After=syslog.target
After=network-online.target
Wants=network-online.target

[Service]
User={{ videoconv_user }}
ExecStart={{ videoconv_dir }}/binaries/videoconv-latest start
Restart=on-failure
WorkingDirectory={{ videoconv_dir }}

[Install]
WantedBy=multi-user.target





